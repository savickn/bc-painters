{
  "name": "bc-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.18.1",
    "npm": "9.8.1"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "start:dev": "cross-env NODE_MODE=SPA NODE_ENV=development BABEL_DISABLE_CACHE=1 nodemon ./entry.js",
    "dev": "npm run clean && npm run start:dev",
    "start:client": "webpack serve --mode development --config webpack.config.client.js --env env=development",
    "build-client": "webpack --config ./webpack.config.client.js --env env=production",
    "build-server": "webpack --config ./webpack.config.server.js --env env=production",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "start:prod": "cross-env NODE_ENV=production node ./entry.js",
    "prod": "cross-env NODE_ENV=production npm run clean && npm run build && npm run start:prod",
    "test:all": "cross-env NODE_ENV=test jest --config=jest.config.js",
    "test:models": "cross-env NODE_ENV=test jest model.spec --config=jest.config.js --detectOpenHandles",
    "test:endpoints": "cross-env NODE_ENV=test jest integration.spec --config=jest.config.js --detectOpenHandles",
    "test:react": "cross-env NODE_ENV=test jest /__tests__/.* --config=jest.config.js --detectOpenHandles",
    "test:e2e": "cross-env NODE_ENV=test",
    "deploy": "firebase deploy --only functions"
  },
  "author": "Nicholas Savickas",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "axios": "^1.6.0",
    "body-parser": "^1.20.2",
    "bootstrap": "^5.3.2",
    "composable-middleware": "^0.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "5.1.6",
    "express": "^4.18.2",
    "express-jwt": "5.3.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.2.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.17.0",
    "serve-favicon": "2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/register": "^7.21.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "copy-webpack-plugin": "4.6.0",
    "css-loader": "^6.7.4",
    "enzyme": "^3.7.0",
    "file-loader": "^6.2.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^29.7.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^2.7.6",
    "mongodb-memory-server": "^9.0.1",
    "moxios": "0.4.0",
    "nodemon": "1.15.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "raw-loader": "0.5.1",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "0.12.1",
    "rimraf": "2.6.3",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-dev-server": "^4.15.1",
    "webpack-hot-middleware": "^2.25.3",
    "webpack-node-externals": "^3.0.0"
  }
}
